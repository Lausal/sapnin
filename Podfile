pod 'SwiftyJSON'
pod 'SDWebImage' #For downloading images asynchronously
pod 'PocketSVG' #Convert SVG to Swift4
pod 'PhoneNumberKit', '~> 2.1' #Format phone number to international format
pod 'ProgressHUD'

end
